<% @events.reverse.each do |event| %>
<table align=center>
  <tr>
     <th colspan="2">
      <%=event.name %>
    </th>
  </tr>
  <tr>
    <td>
      <b>Description: </b><%= event.desc %>
    </td>
    <td>
      <b>Where: </b><%= event.church.name %>
    </td>
  </tr>
  <tr>
    <td colspan=2>
      <b>Address: </b><%= event.church.location %>
    </td>
  </tr>
</table>
<% end %>
